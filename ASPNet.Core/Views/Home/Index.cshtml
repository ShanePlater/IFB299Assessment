@{
    ViewData["Title"] = "Home Page";
}
<body>
    <section id="caroursel">
    <div id="headerCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
        <ol class="carousel-indicators">
            <li data-target="#headerCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#headerCarouself" data-slide-to="1"></li>
            <li data-target="#headerCarousel" data-slide-to="2"></li>
            <li data-target="#headerCarousel" data-slide-to="3"></li>
        </ol>
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src="~/images/banner_ochestra.jpg" alt="orchestra.jpg" class="banner-img"/>
                <div class="carousel-caption" role="option">
                    <h2>Mika's Music School</h2>
                    <p> Bring music to life.</p>
                    <a class="btn btn-default" href="/Session/Register">
                        Join Now
                    </a>
                </div>
            </div>
            <div class="item">
                <img src="~/images/guitar.jpg" alt="guitar.jpg" class="banner-img"/>
                <div class="carousel-caption">
                    <p>1 2 3 a b c</p>
                </div>
            </div>
            <div class="item">
                <img src="~/images/saxophone.jpg" alt="saxophone.jpg" class="banner-img"/>
                <div class="carousel-caption">
                    <p>1 2 3 a b c</p>
                </div>
            </div>
            <div class="item">
                <img src="~/images/piano_player.jpg" alt="piano_player.jpg" class="banner-img"/>
                <div class="carousel-caption">
                    <p>1 2 3 a b c</p>
                </div>
            </div>
        </div>
        <a class="left carousel-control" href="#headerCarousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="##headerCarousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    </section>
   <section id="services">
<div class=" ">
    <article> <h1 class="main-header"> Services </h1></article>
</div>
   </section>
   <section>
       <h1 class="main-header">Testimonials</h1>
       <div id="TestimonyCarousel" class="carousel slide" data-ride="carousel" data-interval="3500">
           <ol class="carousel-indicators">
               <li data-target="#TestimonyCarousel" data-slide-to="0" class="active"></li>
               <li data-target="#TestimonyCarousel" data-slide-to="1"></li>
               <li data-target="#TestimonyCarousel" data-slide-to="2"></li>
               <li data-target="#TestimonyCarousel" data-slide-to="3"></li>
               <li data-target="#TestimonyCarousel" data-slide-to="4"></li>
           </ol>
           <div class="row">
               <div class="col-xs-offset-3 col-xs-6">
                   <div class="carousel-inner" id="carousel-testimony">
                       <div class="item active">
                           <div class="carousel-content">
                               <p>"The team at MMS make learning music an excellent and fun experience perfect for all skill levels. "</p>
                               <h2 class="carousel-author"> Andrew Carr</h2>
                           </div>
                       </div>
                       <div class="item">
                           <div class="carousel-content">
                               <p>"The MMS teachers are really passionate about music and have an understanding of how to tailor lessons to meet each student's technical needs."</p>
                               <h2 class="carousel-author"> Bill Gates </h2>
                           </div>
                       </div>
                       <div class="item">
                           <div class="carousel-content">
                               <p>"I've been learning at MMS for about 3 years and I've learn so much through such a supportive community. "</p>
                               <h2 class="carousel-author"> Peter Parker </h2>
                           </div>
                       </div>
                       <div class="item">
                           <div class="carousel-content">
                               <p>"I've noticed serious improvements in my musical ability since attending MMS. The tutors are amazing and very helpful and all in all, MMS will get you where you want to go."</p>
                               <h2 class="carousel-author"> Mark Zuckerberg </h2>
                           </div>
                       </div>
                       <div class="item">
                           <div class="carousel-content">
                               <p>"Help me learn music, MMS has."</p>
                               <h2 class="carousel-author"> Yoda </h2>
                           </div>
                       </div>


                   </div>
               </div>
           </div>

       </div>
   </section>


</body>